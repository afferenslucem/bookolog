{
  "name": "frontend",
  "version": "0.6.11",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "npm run ci:build-e2e && start-server-and-test ci:start-server http://localhost:4200 ci:run-cypress",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "ci:build-e2e": "ng build --prod --configuration=e2e",
    "ci:start-server": "angular-http-server --path ./dist/frontend -p 4200",
    "ci:run-cypress": "cypress run"
  },
  "private": true,
  "dependencies": {
